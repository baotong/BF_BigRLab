{
    "datadir" : "../data",
    "hasid" : true,
    "autoremove" : true, // 执行完任务后删除输入文件
    "tasks" : [
        {
            "name" : "raw2fv",
            "lib"  : "raw2fv.so",
            "input" : "adult.data.id.1000",
            "desc" : "adult_conf.json",
            "new_desc" : "adult_conf_update.json"
        },
        {
            "name" : "normalize",
            "lib" : "normalize.so"  // 输入用上一步输出，输出文件名自动生成
