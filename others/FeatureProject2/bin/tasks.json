        {
            "name" : "format output xgboost",
            "lib" : "format_output.so",
            "autoremove" : false,   // 覆盖全局设置
            "output" : "adult.id.1000.formatted"
        },
        {
            "name" : "format output xgboost concur",
            "lib" : "format_output_concur.so",
            "output" : "adult.id.1000.formatted.concur",
            "concur_file" : "adult.concur",
            "mark" : "_1"
        }
