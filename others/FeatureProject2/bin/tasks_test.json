{
    "datadir" : "../data",
    "tasks" : [
        {
            "name" : "article2vec",
            "lib" : "article2vec.so",
            "input" : "search_txt",
            "output" : "vectors.txt"
        },
        {
            "name" : "raw2fv",
            "lib"  : "raw2fv.so",
            "input" : "adult.data.id.1000",
            "output" : "adult.fv.id.1000",
            "desc" : "adult_conf_update.json",
            "hasid" : true
        },
        {
            "name" : "normalize",
            "lib" : "normalize.so",
            "output" : "adult.fv.id.1000.normalized"
        },
        {
            "name" : "format output xgboost",
            "lib" : "format_output.so",
            "output" : "adult.id.1000.formatted"
        },
        {
            "name" : "format output xgboost concur",
            "lib" : "format_output_concur.so",
            "input" : "adult.fv.id.1000.normalized",
            "output" : "adult.id.1000.formatted.concur",
            "concur_file" : "adult.concur",
            "mark" : "_1"
        }
    ]
}

